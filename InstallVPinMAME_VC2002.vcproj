<?xml version="1.0" encoding = "Windows-1252"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="InstallVPinMAME"
	SccProjectName=""
	SccLocalPath="">
	<Platforms>
		<Platform
			Name="Win32"/>
	</Platforms>
	<Configurations>
		<Configuration
			Name="Debug|Win32"
			OutputDirectory="obj\VC2002\$(ProjectName)\$(PlatformName)\$(ConfigurationName)"
			IntermediateDirectory="$(OutDir)\Intermediate"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="2">
			<Tool
				Name="VCCLCompilerTool"
				Optimization="0"
				AdditionalIncludeDirectories="$(IntDir)\MIDL"
				PreprocessorDefinitions="WIN32;_DEBUG;_WINDOWS"
				BasicRuntimeChecks="3"
				RuntimeLibrary="5"
				AssemblerListingLocation="$(IntDir)\"
				ObjectFile="$(IntDir)\"
				ProgramDataBaseFileName="$(IntDir)\$(TargetName).pdb"
				BrowseInformation="1"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"
				DebugInformationFormat="4"/>
			<Tool
				Name="VCCustomBuildTool"
				Description="Copying to root..."
				CommandLine="copy &quot;$(TargetPath)&quot; &quot;$(ProjectDir)&quot;
copy &quot;$(TargetDir)$(TargetName).pdb&quot; &quot;$(ProjectDir)&quot;
"
				Outputs="$(ProjectDir)$(TargetFileName)"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalDependencies="odbc32.lib odbccp32.lib version.lib"
				OutputFile="$(OutDir)\$(ProjectName)_VC2002d.exe"
				Version="4.0"
				LinkIncremental="2"
				SuppressStartupBanner="TRUE"
				GenerateDebugInformation="TRUE"
				ProgramDatabaseFile="$(OutDir)\$(TargetName).pdb"
				SubSystem="2"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="_DEBUG"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName="$(InputName).tlb"
				OutputDirectory="$(IntDir)\MIDL"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="_DEBUG"
				Culture="1033"
				ShowProgress="FALSE"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"
				SuppressStartupBanner="TRUE"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
		<Configuration
			Name="Release|Win32"
			OutputDirectory="obj\VC2002\$(ProjectName)\$(PlatformName)\$(ConfigurationName)"
			IntermediateDirectory="$(OutDir)\Intermediate"
			ConfigurationType="1"
			UseOfMFC="0"
			ATLMinimizesCRunTimeLibraryUsage="FALSE"
			CharacterSet="2">
			<Tool
				Name="VCCLCompilerTool"
				InlineFunctionExpansion="1"
				AdditionalIncludeDirectories="$(IntDir)\MIDL"
				PreprocessorDefinitions="WIN32;NDEBUG;_WINDOWS"
				StringPooling="TRUE"
				RuntimeLibrary="4"
				EnableFunctionLevelLinking="TRUE"
				AssemblerListingLocation="$(IntDir)\"
				ObjectFile="$(IntDir)\"
				ProgramDataBaseFileName="$(IntDir)\$(TargetName).pdb"
				WarningLevel="3"
				SuppressStartupBanner="TRUE"/>
			<Tool
				Name="VCCustomBuildTool"
				Description="Copying to root..."
				CommandLine="copy &quot;$(TargetPath)&quot; &quot;$(ProjectDir)&quot;
"
				Outputs="$(ProjectDir)$(TargetFileName)"/>
			<Tool
				Name="VCLinkerTool"
				AdditionalDependencies="odbc32.lib odbccp32.lib version.lib"
				OutputFile="$(OutDir)\$(ProjectName)_VC2002.exe"
				Version="4.0"
				LinkIncremental="0"
				SuppressStartupBanner="TRUE"
				ProgramDatabaseFile="$(OutDir)\$(TargetName).pdb"
				SubSystem="2"
				TargetMachine="1"/>
			<Tool
				Name="VCMIDLTool"
				PreprocessorDefinitions="NDEBUG"
				SuppressStartupBanner="TRUE"
				TargetEnvironment="1"
				TypeLibraryName="$(InputName).tlb"
				OutputDirectory="$(IntDir)\MIDL"/>
			<Tool
				Name="VCPostBuildEventTool"/>
			<Tool
				Name="VCPreBuildEventTool"/>
			<Tool
				Name="VCPreLinkEventTool"/>
			<Tool
				Name="VCResourceCompilerTool"
				PreprocessorDefinitions="NDEBUG"
				Culture="1033"
				ShowProgress="FALSE"/>
			<Tool
				Name="VCWebServiceProxyGeneratorTool"
				SuppressStartupBanner="TRUE"/>
			<Tool
				Name="VCWebDeploymentTool"/>
		</Configuration>
	</Configurations>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;r;odl;idl;hpj;bat">
			<File
				RelativePath="src\instvpm\Globals.cpp">
			</File>
			<File
				RelativePath="src\instvpm\InstallVPinMAME.cpp">
			</File>
			<File
				RelativePath="src\instvpm\InstallVPinMAME.rc">
			</File>
			<File
				RelativePath="src\instvpm\TestVPinMAME.cpp">
			</File>
			<File
				RelativePath="src\instvpm\Utils.cpp">
			</File>
			<Filter
				Name="VPinMAME"
				Filter="">
				<File
					RelativePath="src\win32com\VPinMAME.idl">
					<FileConfiguration
						Name="Debug|Win32">
						<Tool
							Name="VCMIDLTool"
							HeaderFileName="$(InputName).h"/>
					</FileConfiguration>
					<FileConfiguration
						Name="Release|Win32">
						<Tool
							Name="VCMIDLTool"
							HeaderFileName="$(InputName).h"/>
					</FileConfiguration>
				</File>
			</Filter>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl">
			<File
				RelativePath="src\instvpm\Globals.h">
			</File>
			<File
				RelativePath="src\instvpm\TestVPinMAME.h">
			</File>
			<File
				RelativePath="src\instvpm\Utils.h">
			</File>
			<File
				RelativePath="src\instvpm\resource.h">
			</File>
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;rgs;gif;jpg;jpeg;jpe">
			<File
				RelativePath="src\instvpm\Res\InstallVPinMAME.ICO">
			</File>
		</Filter>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
