if(isset($_POST['dip01_v']))
{
$fp = fsockopen("localhost", 5963, $errno, $errstr, 30);
$value = "V_" . $_POST['dip01_v'];
fwrite($fp,$value);
fclose($fp);
$fp = fsockopen("localhost", 5963, $errno, $errstr, 30);
$value = "K_DIP01_" . $_POST['dip01_k'];
fwrite($fp,$value);
fclose($fp);
}
